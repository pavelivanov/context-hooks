{
  "name": "context-hooks",
  "version": "0.0.1",
  "description": "React Hooks for accessing state and dispatch from a store",
  "homepage": "https://github.com/pavelivanov/context-hooks#readme",
  "author": "Pavel Ivanov <grammka@gmail.com> (https://github.com/pavelivanov)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavelivanov/context-hooks.git"
  },
  "keywords": [
    "react",
    "hooks",
    "context",
    "store",
    "action",
    "actions",
    "reducer",
    "dispatch",
    "redux"
  ],
  "main": "./lib",
  "scripts": {
